CREATE TABLE authors (id INTEGER PRIMARY KEY AUTOINCREMENT,
first_name TEXT,
last_name TEXT,
min_estimated_sales INTEGER,
max_estimated_sales INTEGER,
original_language TEXT,
genre_and_or_or_major_works TEXT,
number_of_books INTEGER,
nationality TEXT);
